<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elearneur.edubox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EduBox">
        <activity android:name=".Test"></activity>
        <activity android:name=".NotificationView" />
        <activity android:name=".Notifications" />
        <activity android:name=".GroupCalendar" />
        <activity android:name=".PersonalCalendar" />
        <activity
            android:name=".EventAdd"
            android:parentActivityName=".PersonalCalendar" />
        <activity android:name=".GroupJoin" />
        <activity android:name=".GroupCreate" />
        <activity android:name=".Groups" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>